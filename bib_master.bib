@article{Rignot2006,
	__Markedentry = {[ben]},
	Abstract = {Using satellite radar interferometry observations of Greenland, we
	detected widespread glacier acceleration below 66$\,^{\circ}$ north between
	1996 and 2000, which rapidly expanded to 70$\,^{\circ}$ north in 2005. Accelerated
