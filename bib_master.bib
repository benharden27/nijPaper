	Isbn = {0079-6611},
	Journal = {Progress in Oceanography},
	Keywords = {Arctic Mediterranean; Freshwater fluxes; Ocean circulation; Atlantic meridional overturning circulation},
	Month = {2007/6//},
	Number = {3--4},
	Pages = {210--230},
	Title = {Current estimates of freshwater flux through Arctic and subarctic seas},
	Ty = {JOUR},
