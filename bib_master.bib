	Owner = {ben},
	Pages = {83--116},
	Timestamp = {2009.03.24},
	Title = {Turbulence Structures Over The Marginal Ice Zone Under Flow Parallel To The Ice Edge: Measurements And Parameterizations},
	Volume = {102},
	Year = {2002}}

@article{Druee2001,
	Abstract = {Three aircraft-based studies of boundary-layer fronts (BLFs) werecarried
