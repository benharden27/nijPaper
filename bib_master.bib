	Date-Modified = {2015-06-19 15:43:51 +0000},
	Doi = {10.1016/j.dsr.2013.05.007},
	Isbn = {0967-0637},
	Journal = {Deep Sea Research Part I: Oceanographic Research Papers},
	Keywords = {Denmark Strait; East Greenland Current; North Icelandic Jet; Blosseville Basin; Denmark Strait Overflow Water; Arctic freshwater export},
	Number = {0},
	Pages = {20--39},
	Title = {{Revised circulation scheme north of the Denmark Strait}},
	Ty = {JOUR},
