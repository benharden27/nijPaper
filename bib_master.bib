	Journal = {Deep Sea Research Part I: Oceanographic Research Papers},
	Keywords = {Trimethylamine oxide (TMAO); Lipid; Chemical composition; Hydrostatic pressure; Depth; Hawaiian fish},
	Month = {6},
	Pages = {37--44},
	Title = {Trimethylamine oxide accumulation as a function of depth in Hawaiian mid-water fishes},
	Ty = {JOUR},
