@article{Aagaard1989,
	Abstract = {Salinity stratification is critical to the vertical circulation of
	the high-latitude ocean. We here examine the control of the vertical
	circulation in the northern seas, and the potential for altering
	it, by considering the budgets and storage of fresh water in the
	Arctic Ocean and in the convective regions to the south. We find
	that the present-day Greenland and Iceland seas, and probably also
	the Labrador Sea, are rather delicately poised with respect to their
	ability to sustain convection. Small variations in the fresh water
	supplied to the convective gyres from the Arctic Ocean via the East
	Greenland Current can alter or stop the convection in what may be
	a modern analog to the halocline catastrophes proposed for the distant
	past. The North Atlantic salinity anomaly of the 1960s and 1970s
	is a recent example; it must have had its origin in an increased
	fresh water discharge from the Arctic Ocean. Similarly, the freshening
	and cooling of the deep North Atlantic in recent years is a likely
	manifestation of the increased transfer of fresh water from the Arctic
	Ocean into the convective gyres. Finally, we note that because of
	the temperature dependence of compressibility, a slight salinity
	stratification in the convective gyres is required to efficiently
	ventilate the deep ocean.},
	Author = {Aagaard, K. and Carmack, E. C.},
	Date-Added = {2013-12-22 23:14:14 +0000},
	Date-Modified = {2013-12-22 23:14:14 +0000},
	File = {:/local-home/vfm08zxu/Documents/Papers/aargard_freshwater.pdf:PDF},
	Issn = {0148-0227},
	Journal = {J. Geophys. Res.},
	Number = {C10},
	Owner = {vfm08zxu},
	Pages = {14485--14498},
	Publisher = {AGU},
	Timestamp = {2012.02.07},
	Title = {The Role of Sea Ice and Other Fresh Water in the Arctic Circulation},
	Url = {http://dx.doi.org/10.1029/JC094iC10p14485},
	Volume = {94},
	Year = {1989},
	Bdsk-Url-1 = {http://dx.doi.org/10.1029/JC094iC10p14485}}

