	Journal = {Deep Sea Research Part I: Oceanographic Research Papers},
	Keywords = {U37K′; TEXH86; Sediment trap; Upwelling system; Indonesia; Indian Ocean},
	Month = {6},
	Pages = {14--24},
	Title = {Concentrations and abundance ratios of long-chain alkenones and glycerol dialkyl glycerol tetraethers in sinking particles south of Java},
	Ty = {JOUR},
