	Keywords = {Antarctic primnoids; Longevity; Growth rings; Radioisotope dating; Radial Growth Rates},
	Month = {6},
	Pages = {57--67},
	Title = {New insights on Antarctic gorgonians' age, growth and their potential as paleorecords},
	Ty = {JOUR},
