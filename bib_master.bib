	Isbn = {0967-0637},
	Journal = {Deep Sea Research Part I: Oceanographic Research Papers},
	Month = {6},
	Pages = {137--154},
	Title = {Structure, transport, and vertical coherence of the Gulf Stream from the Straits of Florida to the Southeast Newfoundland Ridge},
	Ty = {JOUR},
