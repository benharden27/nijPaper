	Date-Modified = {2017-07-27 14:18:39 +0000},
	Doi = {https://doi.org/10.1016/j.dsr.2015.10.010},
	Isbn = {0967-0637},
	Journal = {Deep Sea Research Part I: Oceanographic Research Papers},
	Keywords = {ANME; Methanogens; Methanotrophs; Deep-sea},
	Month = {6},
	Pages = {45--56},
	Title = {Discovery of a chemosynthesis-based community in the western South Atlantic Ocean},
	Ty = {JOUR},
