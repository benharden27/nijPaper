	Pages = {18059--18072},
	Publisher = {AGU},
	Timestamp = {2012.01.05},
	Title = {Structure and transports of the East Greenland Current at 75�N from moored current meters},
	Url = {http://dx.doi.org/10.1029/1999JC900146},
	Volume = {104},
