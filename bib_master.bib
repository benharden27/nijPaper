	Journal = {J. Geophys. Res.},
	Keywords = {latent heat flux climatologies, atmospheric energy cycle, atmospheric hydrological cycle, latent heat flux parameterization, 3339 Atmospheric Processes: Ocean/atmosphere interactions, 3309 Atmospheric Processes: Climatology, 1626 Global Change: Global climate models, 3307 Atmospheric Processes: Boundary layer processes},
	Month = mar,
	Owner = {ben},
	Pages = {--},
	Publisher = {American Geophysical Union},
	Timestamp = {2009.09.15},
	Title = {Oceanic latent heat fluxes: Consistency with the atmospheric hydrological and energy cycles and general circulation modeling},
	Volume = {112},
	Year = {2007}}

