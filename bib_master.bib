	Journal = {Geophys. Res. Lett.},
	Keywords = {buoy observations, tip jets, Cape Farewell, 3339 Atmospheric Processes: Ocean/atmosphere interactions, 3307 Atmospheric Processes: Boundary layer processes, 0312 Atmospheric Composition and Structure: Air/sea constituent fluxes, 3349 Atmospheric Processes: Polar meteorology, 4247 Oceanography: General: Marine meteorology},
	Month = sep,
	Owner = {ben},
	Pages = {L18802},
	Publisher = {American Geophysical Union},
	Timestamp = {2009.07.15},
	Title = {{Buoy observations from the windiest location in the world ocean, Cape Farewell, Greenland}},
	Volume = {35},
	Year = {2008},
	Bdsk-Url-1 = {http://dx.doi.org/10.1029/2008GL034845}}

@article{Moore2005,
	Abstract = {The high topography of Greenland results in a number of orographically
	induced high wind speed flows along its coast that are of interest
	from both a severe weather and climate perspective. Here the surface
	wind field dataset from the NASA&#8211;JPL SeaWinds scatterometer
	on board the Quick Scatterometer (QuikSCAT) satellite is used to
	develop a wintertime climatology of these flows. The high spatial
	resolution and the twice-daily sampling of the SeaWinds instrument
	allows for a much more detailed view of the surface winds around
	Greenland than has been previously possible. Three phenomena stand
	out as the most distinctive features of the surface wind field during
	the winter months: the previously identified tip jets and reverse
	tip jets, as well as the hitherto unrecognized barrier flows along
	its southeast coast in the vicinity of the Denmark Strait. Peak surface
	wind speeds associated with these phenomena can be as large as 50
	m s&#8722;1 with winds over 25 m s&#8722;1 occurring approximately
	10&#37;&#8211;15&#37; of the time at each location.A compositing
	technique is used to show that each type of flow is the result of
	an interaction between a synoptic-scale parent cyclone and the high
	topography of Greenland. In keeping with previous work, it is argued
	that tip jets are caused by a combination of conservation of the
	Bernoulli function during orographic descent and acceleration due
	to flow splitting as stable air passes around Cape Farewell, while
	barrier winds are a geostrophic response to stable air being forced
	against high topography. It is proposed that reverse tip jets occur
	when barrier winds reach the end of the topographic barrier and move
	from a geostrophic to a gradient wind balance, becoming supergeostrophic
	as a result of their anticyclonic curvature.},
	Author = {Moore, G. W. K. and Renfrew, I. A.},
	Date-Added = {2013-12-22 23:14:14 +0000},
	Date-Modified = {2014-02-09 18:03:32 +0000},
	Doi = {10.1175/JCLI3455.1},
	File = {:/local-home/vfm08zxu/Documents/Papers/renfrew-moore-wind-climatology.pdf:PDF},
	Journal = {Journal of Climate},
	Month = sep,
	Number = {18},
	Owner = {ben},
	Pages = {3713--3725},
	Timestamp = {2008.11.24},
	Title = {{Tip Jets and Barrier Winds: A QuikSCAT Climatology of High Wind Speed Events around Greenland}},
	Volume = {18},
	Year = {2005},
	Bdsk-Url-1 = {http://dx.doi.org/10.1175/JCLI3455.1}}

