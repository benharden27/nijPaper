	Journal = {J. Geophys. Res.},
	Keywords = {upwelling, 0312 Atmospheric Composition and Structure: Air/sea constituent fluxes, 3339 Atmospheric Processes: Ocean/atmosphere interactions, 4528 Oceanography: Physical: Fronts and jets, 4512 Oceanography: Physical: Currents},
	Month = sep,
	Owner = {ben},
	Pages = {--},
	Publisher = {American Geophysical Union},
	Timestamp = {2010.03.03},
	Title = {Upwelling on the continental slope of the Alaskan Beaufort Sea: Storms, ice, and oceanographic response},
	Volume = {114},
	Year = {2009}}

