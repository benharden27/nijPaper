@article{Moore2009,
	Abstract = {Due to its high topography, Greenland results in significant distortion
	to the atmospheric flow affecting local and remote weather systems
	and, via air-sea interaction processes, the coupled climate system.
	During February and March 2007 a field campaign was carried out to
	help document this flow distortion as part of the international research
	project called the Greenland Flow Distortion Experiment (GFDex).
	In this paper, we use a variety of meteorological datasets to describe
	surface and upper-air conditions during the GFDex experimental period
	and compare them to a long-term climatology so as to provide a context
	for the aircraft-based observations that were collected. We show
	that the observational program occurred during anomalous conditions
	relative to the climatological mean. While the North Atlantic Oscillation
	index was positive, there was a southern shift in the centre of the
	Icelandic Low and a more zonally-oriented stormtrack. It is suggested
	that a weakening in the Canadian Polar Trough and the concomitant
	warming throughout the troposphere in the Northwestern Atlantic contributed
	to these anomalous conditions. As a result, there was more northerly
	flow in the Labrador Sea and sustained northeasterly flow along the
	southeast coast of Greenland that favoured the formation of barrier
	flow and reverse tip jets, while inhibiting the formation of tip
	jets. The impacts of these synoptic-scale conditions on the associated
	air-sea fluxes are discussed.},
	Author = {Moore, G. W. K. and Renfrew, I. A. and Pickart, R. S.},
	Date-Added = {2013-12-22 23:14:14 +0000},
	Date-Modified = {2013-12-22 23:14:14 +0000},
	File = {:/local-home/vfm08zxu/Documents/Papers/moore-pickart-renfrew-gfdex-clim.pdf:PDF},
	Journal = {Quarterly Journal of the Royal Meteorological Society},
	Owner = {ben},
	Timestamp = {2009.03.26},
	Title = {Climatological conditions for the subpolar North Atlantic during the field campaign of the Greenland Flow Distortion Experiment},
	Year = {2009}}

@article{Murray2010,
	Abstract = {Synchronous acceleration and thinning of southeast (SE) Greenland
	glaciers during the early 2000s was the main contributor that resulted
	in the doubling of annual discharge from the ice sheet. We show that
	this acceleration was followed by a synchronized and widespread slowdown
	of the same glaciers, in many cases associated with a decrease in
	thinning rates, and we propose that ice sheet--ocean interactions
	are the first-order regional control on these recent mass changes.
	Sea surface temperature and mooring data show that the preceding
	dynamic thinning coincides with a brief decline in the cold East
	Greenland Coastal Current (EGCC) and East Greenland Current. We suggest
	this decline was partly induced by a reduction in ice sheet runoff,
	which allowed warm water from the Irminger Current to reach the SE
	Greenland coast. A restrengthening of the cold waters coincides with
	the glaciers' subsequent slowdown. We argue that this warming and
	subsequent cooling of the coastal waters was the cause of the glaciers'
	dynamic changes. We further suggest that the restrengthening of the
	EGCC resulted in part from cold water input by increased glacier
	calving during the speedup and increased ice sheet runoff. We hypothesize
	that the main mechanism for ice sheet mass loss in SE Greenland is
	highly sensitive to ocean conditions and is likely subject to negative
	feedback mechanisms.},
	Author = {Murray, T. and Scharrer, K. and James, T. D. and Dye, S. R. and Hanna, E. and Booth, A. D. and Selmes, N. and Luckman, A. and Hughes, A. L. C. and Cook, S. and Huybrechts, P.},
	Date-Added = {2013-12-22 23:14:14 +0000},
	Date-Modified = {2014-05-07 18:18:44 +0000},
	Doi = {10.1029/2009JF001522},
	Issn = {2156-2202},
	Journal = {Journal of Geophysical Research: Earth Surface},
	Keywords = {SE Greenland outlet glaciers, oceanography, ice flow rate},
	Owner = {ben},
	Pages = {F03026},
	Timestamp = {2013.01.22},
	Title = {{Ocean regulation hypothesis for glacier dynamics in southeast Greenland and implications for ice sheet mass changes}},
	Url = {http://dx.doi.org/10.1029/2009JF001522},
	Volume = {115},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1029/2009JF001522}}

