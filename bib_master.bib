	Keywords = {Internal tides; Turbulence; Mixing; Parameterization; Mid-atlantic ridge},
	Month = {6},
	Pages = {79--93},
	Title = {Finescale parameterizations of energy dissipation in a region of strong internal tides and sheared flow, the Lucky-Strike segment of the Mid-Atlantic Ridge},
