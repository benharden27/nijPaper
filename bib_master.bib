	Keywords = {North Atlantic Ocean; subpolar mode water; North Atlantic Oscillation; 4283 Water masses; 4215 Climate and interannual variability; 4504 Air/sea interactions; 4532 General circulation; 4572 Upper ocean and mixed layer processes},
	Number = {C4},
	Pages = {C04016},
	Title = {{Interannual variability of the Subpolar Mode Water properties over the Reykjanes Ridge during 1990--2006}},
