	Isbn = {2169-9291},
	Journal = {Journal of Geophysical Research: Oceans},
	Journal1 = {J. Geophys. Res. Oceans},
	Keywords = {Denmark Strait overflow; transport calculation; ADCP; 4553 Overflows; 4594 Instruments and techniques; 4532 General circulation; 4215 Climate and interannual variability; 4294 Instruments and techniques},
	Number = {4},
	Pages = {3434--3450},
	Title = {Revised transport estimates of the Denmark Strait overflow},
